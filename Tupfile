WX_COMPILE_FLAGS = -I/usr/local/lib/wx/include/gtk3-unicode-3.3 -I/usr/local/include/wx-3.3 -D_FILE_OFFSET_BITS=64 -DWXUSINGDLL -D__WXGTK__ -pthread
WX_LINK_FLAGS = -L/usr/local/lib -pthread   -lwx_gtk3u_xrc-3.3 -lwx_gtk3u_html-3.3 -lwx_gtk3u_qa-3.3 -lwx_gtk3u_core-3.3 -lwx_baseu_xml-3.3 -lwx_baseu_net-3.3 -lwx_baseu-3.3

: src/hellowx.cpp |> ^j^ g++ src/hellowx.cpp -o build/hello $(WX_COMPILE_FLAGS) $(WX_LINK_FLAGS)  |> build/hello
